    "bcrypt": "^5.1.0",
    "buffer-to-stream": "^1.0.0",
    "chaca": "^1.8.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "mongoose": "^6.8.3",
    "passport": "^0.6.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth2": "^0.2.0",
